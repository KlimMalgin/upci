{
    "presets": [
        "react",
        [
            "env",
            {
                "modules": false,
                "import": false
            }
        ],
        "es2016",
        "es2017"
    ],
    "plugins": [
        "transform-object-rest-spread",
        "transform-es2015-destructuring",
        "syntax-dynamic-import"
    ],
    "env": {
        "test":{
            "plugins": [
                "dynamic-import-node",
                "transform-es2015-modules-commonjs"
            ]
        }
    }
}
